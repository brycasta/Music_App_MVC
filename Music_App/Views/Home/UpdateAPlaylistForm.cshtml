<h5>Playlist Id: @ViewBag.Playlists[0].PlaylistId</h5>

<form action="UpdateAPlaylist" method="post">
    <input type="hidden" name="playlistId" value="@ViewBag.Playlists[0].PlaylistId" />

    Playlist Name<br />
    <input type="text" name="playlistName" value="@ViewBag.Playlists[0].PlaylistName" /><br /><br />

    Description<br />
    <input type="text" name="description" value="@ViewBag.Playlists[0].Description" /><br /><br />

    Created Date<br />
    <input type="datetime-local" name="createdDate" value="@ViewBag.Playlists[0].CreatedDate" /><br /><br />

    <input type="submit" />
    <input type="reset" />
</form>
